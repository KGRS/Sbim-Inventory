	<queryString>
		<![CDATA[SELECT
     Suppliers."SupplierName" AS Suppliers_SupplierName,
     Suppliers."SupplierCode" AS Suppliers_SupplierCode,
     PurchaseOrderMain."PODate" AS PurchaseOrderMain_PODate,
     PurchaseOrderMain."POID" AS PurchaseOrderMain_POID,
     PurchaseOrderMain."SupplierCode" AS PurchaseOrderMain_SupplierCode,
     PurchaseOrderMain."IsPending" AS PurchaseOrderMain_IsPending
FROM
     "dbo"."Suppliers" Suppliers INNER JOIN "dbo"."PurchaseOrderMain" PurchaseOrderMain ON Suppliers."SupplierCode" = PurchaseOrderMain."SupplierCode"
WHERE Suppliers."SupplierName" LIKE 'a%' AND
      PurchaseOrderMain."IsPending" = 'Yes'
]]>
